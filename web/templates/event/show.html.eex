<h2>Show event</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @event.name %>
  </li>
  <%= for agenda_session <- @event.agenda_sessions do %>
    <li><%= agenda_session.name %>- <%= EventilAgenda.AgendaSession.event_type(agenda_session) %>
  <% end %>
</ul>
<%= link "Back", to: event_path(@conn, :index) %>
